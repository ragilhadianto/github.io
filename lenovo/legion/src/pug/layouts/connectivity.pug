            path(d='M-20321.867,1032.28V680.8h65.068' transform='translate(20323.367 -679.3)' fill='none' stroke='#fff' stroke-linecap='round' stroke-width='3')
        .connectivity__svg__usb-right
          svg(xmlns='http://www.w3.org/2000/svg' width='68.068' height='354.48' viewBox='0 0 68.068 354.48')
            path(d='M-20256.8,1032.28V680.8h-65.068' transform='translate(20323.365 -679.3)' fill='none' stroke='#fff' stroke-linecap='round' stroke-width='3')
=`\n`
