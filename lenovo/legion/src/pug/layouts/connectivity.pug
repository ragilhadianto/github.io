      .connectivity__img
        =`\n`
        img.connectivity__img__el(src=data.img, alt='Hassle-free Connectivity.')/
      .connectivity__svg
        .connectivity__svg__power-btn
          p Power Button
          =`\n`
          svg(xmlns='http://www.w3.org/2000/svg' width='3' height='434.088' viewBox='0 0 3 434.088')
            path(d='M-19569.416,1160.893V729.8' transform='translate(19570.916 -728.305)' fill='none' stroke='#fff' stroke-linecap='round' stroke-width='3')
        .connectivity__svg__audio-jack-1
          p
            span 2 x
            span Audio Jack
          =`\n`
          svg(xmlns='http://www.w3.org/2000/svg' width='3' height='223.801' viewBox='0 0 3 223.801')
            path(d='M-19775.5,1018.933v-220.8' transform='translate(19777 -796.633)' fill='none' stroke='#fff' stroke-linecap='round' stroke-width='3')
        .connectivity__svg__audio-jack-2
